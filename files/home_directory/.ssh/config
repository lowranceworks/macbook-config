# uses work ssh-key by default
Host github
    HostName github.com
    User git
    IdentityFile ~/.ssh/work
    IdentitiesOnly yes

# uses personal ssh-key if the remote origin hostname is github-personal (git@github-personal:lowranceworks/Learning.git)
Host github-personal
    HostName github.com
    User git
    IdentityFile ~/.ssh/lowranceworks
    IdentitiesOnly yes